{
  "expo": {
    "name": "AI 키오스크",
    "slug": "ai-kiosk-app", 
    "version": "1.0.0",
    "owner": "mgms",
    "projectId": "e6ec0385-068b-45bd-b7a5-b75acb83ab5e",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aikioskapp.kiosk",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "CAMERA", 
        "RECORD_AUDIO",
        "WAKE_LOCK",
        "SYSTEM_ALERT_WINDOW"
      ],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-screen-orientation"
    ],
    "extra": {
      "eas": {
        "projectId": "e6ec0385-068b-45bd-b7a5-b75acb83ab5e"
      }
    }
  }
}
